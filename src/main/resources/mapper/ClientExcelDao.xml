<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.zj.dao.ClientExcelDao">

    <select id="findAll" resultType="com.zj.entity.ClientStudent">

        select * from client_student

    </select>

    <insert id="save" parameterType="com.zj.entity.ClientStudent">

        INSERT INTO client_student (name, age, arre)  VALUES (#{name}, #{age}, #{arre})

    </insert>

</mapper>
